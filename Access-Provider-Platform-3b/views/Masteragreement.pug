doctype html
head
 link(rel="icon", href="/images/favicon.ico")
 style.
  body { 
    margin: 0;  
    padding: 0;  
    background-color:#ffd699;
    font-family: 'Arial';  
  } 
  h1,h2,{
    text-align:center;

    }
  table, th, td {
  border:1px solid blue;
  }

body
  block content
  script(src='http://code.jquery.com/jquery-3.3.1.min.js')
  script(src='/javascripts/app.js')
h1 Access Management Platform 
h2 Agreement

table#agreementtable(style='width:100%')
  tr
    th ID
    th Agreement Name
    th Details

  each user in users
    tr
    td #{user.id}
    td #{user.title}
    td 
     li
      a(href=`/users/agreementdetails?id=${user.id}&title=${user.title}&position=${user.position}&skill=${user.skill}&salary=${user.salary}&cycle=${user.cycle}&jobStartDate=${user.jobStartDate}&jobEndDate=${user.jobEndDate}&technologyLevel=${user.technologyLevel}&role=${user.role}&status=${user.status}`) Full Agreement Details