doctype html
head
 link(rel="icon", href="/images/favicon.ico")
 style.
  body { 
    margin: 0;  
    padding: 0;  
    background-color:#6abadeba;
    font-family: 'Arial';  
  } 
  h1,h2,{
    text-align:center;

    }
  table, th, td {
  border:1px solid blue;
  }

body
  block content
  script(src='http://code.jquery.com/jquery-3.3.1.min.js')
  script(src='/javascripts/app.js')
h1 Access Management Platform 
h2 Agreement Bidding Details

table#agreementdetailstable(style='width:100%')
  tr
    th Position
    th Id
    th Title
    th Details 

  each user in users
    tr
    td #{user.position}
    td #{user.id}
    td #{user.title}
    td 
      a(href=`/users/biddingdetails?id=${user.id}&title=${user.title}&position=${user.position}&skill=${user.skill}&salary=${user.salary}&cycle=${user.cycle}&jobStartDate=${user.jobStartDate}&jobEndDate=${user.jobEndDate}&technologyLevel=${user.technologyLevel}&role=${user.role}&status=${"accepted"}`) Full Details